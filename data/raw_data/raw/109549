                    <script src="https://maps.googleapis.com/maps/api/js?sensor=false"></script><script>
                        function codeAddress(address) {
                            var geocoder = new google.maps.Geocoder();
                            var maptype = google.maps.MapTypeId.ROADMAP
                            geocoder.geocode({'address':address}, function(results, status) {
                                if (status == google.maps.GeocoderStatus.OK) {
                                    var position = results[0].geometry.location;
                                    var mapOptions = {
                                        zoom:10,
                                        center:position,
                                        mapTypeId:maptype
                                    }
                                    var map = new google.maps.Map(document.getElementById('map-canvas'), mapOptions);
                                    var markerOptions = {
                                        map:map,
                                        position:position,
                                        title:address
                                    }
                                    var marker = new google.maps.Marker(markerOptions);
                                    var infowindowOptions = {
                                        content: address,
                                        maxWidth: 400
                                    }
                                    var infowindow = new google.maps.InfoWindow(infowindowOptions);
                                    infowindow.open(map,marker);
                                } else {
                                    throw('Geocode was not successful for the following reason: ' + status);
                                }
                            });
                        }
                    </script><script>$(function() {codeAddress("Richard Doll Building, Old Road Campus, Headington")})</script><!-- START header --><div class="row">
