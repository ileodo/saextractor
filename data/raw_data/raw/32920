  $("span.downarr a, span.showbar a").click(function() { return false; });	
	var openState = Get_Cookie('openstate');
	if(openState != null){ if(openState == 'closed'){HideMenuToolbar();} if(openState == 'open'){ShowMenuToolbar();}}
});	
</script>


